<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>io.carbon</string>

    <key>Program</key>
    <string>/usr/local/bin/carbon</string>

    <key>Nice</key>
    <integer>-20</integer>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/carbon.err</string>

    <key>StandardOutPath</key>
    <string>/tmp/carbon.out</string>

    <key>ProcessType</key>
    <string>Standard</string>
  </dict>
</plist>
